{
  "name": "friendlymessage",
  "version": "1.0.0",
  "description": "Messenger",
  "main": "./index.js",
  "scripts": {
    "dev": "parcel ./static/index.html",
    "build": "parcel build ./static/index.html --no-source-maps --no-cache",
    "start": "parcel ./static/index.html --port 3000 && node --unhandled-rejection=strict ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tengy83/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [
    "messenger"
  ],
  "author": "Evgenii Mizerovskii",
  "license": "ISC",
  "devDependencies": {
    "express": "^4.17.1",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "sass": "^1.35.1"
  },
  "engines": {
    "node": "14.17.0"
  },
  "dependencies": {
    "core-js": "^3.15.2"
  },
  "parcelCleanPaths": [
    ".cache",
    "dist"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "static/img/",
        "staticOutDir": "/img/"
      }
    ]
  }
}
