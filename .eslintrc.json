{
  "plugins": ["prettier", "unicorn"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "quotes": [2, "single", { "avoidEscape": true }]
  },
  "extends": ["eslint:recommended", "google"]
}
